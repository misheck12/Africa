<h1>All <%= @user.name %>'s Posts</h1>

<%= render @user %>

<% @post.each do |post| %>
    <%= render post %>
    <div class='comments'>
        <% post.last_five_comments.each do |comment| %>
        <p><%= comment.author.name %>: <%= comment.text %></p>
        <%end%>
    </div>
<%end%>

<button>Pagination</button>