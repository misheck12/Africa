
</div>
<div class="users-ctn">
  <% @users.each do |user| %>
    <div class="user-ctn">
      <%#= image_tag( user.photo, size: "50x50") %>
      <img src="<%= user.photo %>" alt="image">
      <%= link_to user_path(user.id), class: "link-info" do %>
        <div class="user-l-info">
          <h5><%= user.name %></h5>
          <h5>Number of posts : <%= user.posts_counter %></h5>
        </div>
      <% end %>
    </div>
  <% end %>
</div>